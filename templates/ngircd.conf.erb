[Global]
  Name = <%= @servername %>
  Info = <%= @info %>
  Listen = <%= @listen %>
  Ports  = <%= @ports %>
  AdminInfo1 = <%= @admindesc %>
  AdminInfo2 = <%= @adminlocation %>
  AdminEMail = <%= @adminemail %>
  <% if @motd %>Motd = <%= @motd %><% end -%>
  <% if @motdfile %>Motdfile = <%= @motd %><% end -%>
  <% if @password %>Password = <%= @password %><% end -%>
  <% if @customglobalvars %><% @customglobalvars.each do |key,value| -%>
  <%= key %> = <%= value %>
  <% end %><% end -%>

<% if @options -%>
[Options]
<%= @options.each do |key,value| -%>
<%=key %> = <%= value %>
<% end %><% end -%>

<% @operators.each do |operator| -%>
[Operator]
<% operator.each do |key,value| -%>
  <%= key %> = <%= value %>
<% end -%><% end -%>

<% @servers.each do |server| -%>
[Server]
<% server.each do |key,value| -%>
  <%= key %> = <%= value %>
<% end -%><% end -%>

<% @channels.each do |channel| -%>
[Channel]
<% channel.each do |key,value| -%>
  <%= key %> = <%= value %>
<% end -%><% end -%>
